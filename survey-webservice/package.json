{
	"name": "voomsgo",
	"version": "1.0.0",
	"description": "The SurveyApp backend service",
	"main": "app.ja",
	"engines": {
		"node": "10.4.1",
		"npm": "6.1.0"
	},
	"scripts": {
		"prestart": "npm run -s build",
		"start": "node dist/app",
		"dev": "nodemon --exec \"npm run lint && babel-node src/app.js\"",
		"build": "babel src --out-dir dist",
		"lint": "eslint -c .eslintrc src",
		"test": "npm run test:auth",
		"test:auth": "mocha --opts ./mocha.opts __test__/e2e/**/*.spec.js",
		"test:current": "mocha --opts ./mocha.opts __test__/e2e/rest/auth/google.spec.js"
	},
	"keywords": [
		"Advance",
		"api",
		"nodejs",
		"express"
	],
	"author": "Emmanuel Okafor",
	"license": "ISC",
